<!-- dual manifold pressure -->
<PropertyList>
    <path>FuelFlow.ac</path>
    
    <animation>
        <type>noshadow</type>
        <object-name>FuelFlow</object-name>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>Face</object-name>
        <object-name>Left</object-name>
        <object-name>Right</object-name>
		<emission>
			<red>1.0</red>
			<green>0.8</green>
			<blue>0.4</blue>
			<factor>
				<expression>
					<sum>
						<product>
							<property>sim/model/lights/uv-instrument[0]/brightness-norm</property>
							<value>0.38</value>
						</product>
						<product>
							<property>sim/model/lights/uv-instrument[1]/brightness-norm</property>
							<value>0.62</value>
						</product>
					</sum>
				</expression>
			</factor>
		</emission>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>Left</object-name>
        <property>engines/engine[0]/fuel-flow-gph</property>
        <interpolation>
            <entry><ind> 0.0</ind><dep>   0.0</dep></entry>
            <entry><ind> 8.0</ind><dep> -61.0</dep></entry>
            <entry><ind> 9.0</ind><dep> -67.0</dep></entry>
            <entry><ind>16.50</ind><dep> -74.0</dep></entry>
            <entry><ind>18.0</ind><dep> -83.0</dep></entry>
            <entry><ind>21.00</ind><dep> -91.0</dep></entry>
            <entry><ind>22.00</ind><dep>-101.0</dep></entry>
            <entry><ind>23.0</ind><dep>-112.0</dep></entry>
            <entry><ind>25.0</ind><dep>-112.0</dep></entry>
            <entry><ind>25.75</ind><dep>-123.0</dep></entry>
            <entry><ind>26.5</ind><dep>-133.0</dep></entry>
            <entry><ind>27.25</ind><dep>-144.0</dep></entry>
            <entry><ind>29.0</ind><dep>-156.0</dep></entry>
            <entry><ind>30.0</ind><dep>-167.0</dep></entry>
            <entry><ind>31.0</ind><dep>-180.0</dep></entry>
            <entry><ind>31.5</ind><dep>-260.0</dep></entry>
        </interpolation>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>Right</object-name>
        <property>engines/engine[1]/fuel-flow-gph</property>
        <interpolation>
            <entry><ind> 0.0</ind><dep>   0.0</dep></entry>
            <entry><ind> 8.0</ind><dep> -61.0</dep></entry>
            <entry><ind> 9.0</ind><dep> -67.0</dep></entry>
            <entry><ind>16.50</ind><dep> -74.0</dep></entry>
            <entry><ind>18.0</ind><dep> -83.0</dep></entry>
            <entry><ind>21.00</ind><dep> -91.0</dep></entry>
            <entry><ind>22.00</ind><dep>-101.0</dep></entry>
            <entry><ind>23.0</ind><dep>-112.0</dep></entry>
            <entry><ind>25.0</ind><dep>-112.0</dep></entry>
            <entry><ind>25.75</ind><dep>-123.0</dep></entry>
            <entry><ind>26.5</ind><dep>-133.0</dep></entry>
            <entry><ind>27.25</ind><dep>-144.0</dep></entry>
            <entry><ind>29.0</ind><dep>-156.0</dep></entry>
            <entry><ind>30.0</ind><dep>-167.0</dep></entry>
            <entry><ind>31.0</ind><dep>-180.0</dep></entry>
            <entry><ind>31.5</ind><dep>-260.0</dep></entry>
        </interpolation>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>noshadow</type>
    </animation>
    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>10</max-m>
    </animation>

    <effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>Glass</object-name>
	</effect>


</PropertyList>
